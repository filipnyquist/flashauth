{
  "name": "flashauth",
  "version": "0.1.0",
  "author": "Filip Nyquist",
  "main": "dist/index.js",
  "devDependencies": {
    "@types/bun": "^1.1.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
